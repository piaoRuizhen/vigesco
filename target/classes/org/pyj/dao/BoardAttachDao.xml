<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.pyj.dao.BoardAttachDao">
  	<insert id="BoardAttachWrite">	
		insert into tbl_attach(uuid,fileName,uploadPath,image,bno)
		values(#{uuid},#{fileName},#{uploadPath},#{image},#{bno})
  	</insert>
 </mapper>